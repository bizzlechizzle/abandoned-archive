{
  "name": "@abandoned-archive/desktop",
  "version": "0.1.0-alpha",
  "description": "Document abandoned places with verifiable, local-first evidence",
  "author": "Abandoned Archive",
  "license": "MIT",
  "type": "module",
  "main": "dist-electron/main/index.js",
  "scripts": {
    "postinstall": "electron-rebuild -f -o sharp",
    "dev": "vite",
    "build": "vite build",
    "build:electron": "electron-builder --config electron-builder.config.json && pnpm run rebuild",
    "build:all": "pnpm run build && pnpm run build:electron",
    "dist": "electron-builder --mac --config electron-builder.config.json",
    "dist:arm64": "electron-builder --mac --arm64 --config electron-builder.config.json",
    "dist:x64": "electron-builder --mac --x64 --config electron-builder.config.json",
    "preview": "vite preview",
    "electron:dev": "concurrently \"vite\" \"electron .\"",
    "rebuild": "electron-rebuild -f -o sharp",
    "test": "vitest",
    "lint": "eslint src electron"
  },
  "dependencies": {
    "@aa/core": "workspace:*",
    "@aa/services": "workspace:*",
    "@aws-sdk/client-s3": "^3.940.0",
    "@skeletonlabs/skeleton": "^4.4.1",
    "@skeletonlabs/tw-plugin": "^0.4.1",
    "@tanstack/svelte-virtual": "^3.13.12",
        "blake3": "^2.1.7",
    "bufferutil": "^4.0.9",
    "chrono-node": "^2.7.7",
    "exiftool-vendored": "^33.2.0",
    "fluent-ffmpeg": "^2.1.2",
        "leaflet": "^1.9.4",
    "leaflet.vectorgrid": "^1.3.0",
    "node-postal": "^1.3.0",
    "onnxruntime-node": "^1.21.0",
    "p-queue": "^8.0.1",
    "puppeteer-core": "^24.33.0",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "sharp": "^0.33.1",
    "sharp-phash": "^2.2.0",
    "shoemaker": "workspace:*",
    "socket.io-client": "^4.8.3",
    "supercluster": "^8.0.1",
    "tesseract.js": "^5.1.1",
    "utf-8-validate": "^6.0.5",
    "wake-n-blake": "workspace:*",
    "ws": "^8.18.3",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@electron/rebuild": "^3.7.0",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
        "@types/leaflet": "^1.9.8",
    "@types/unzipper": "^0.10.11",
    "@types/ws": "^8.18.1",
    "@xmldom/xmldom": "^0.8.11",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "electron": "35.7.5",
    "electron-builder": "^24.9.1",
    "postcss": "^8.4.32",
    "svelte": "^5.0.0",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "unzipper": "^0.12.3",
    "vite": "^5.4.21",
    "vite-plugin-electron": "^0.28.2",
    "vitest": "^1.1.0"
  }
}
